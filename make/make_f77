include make.inc

MYSRC= $(wildcard $(SRCDIR)/$(SUB)/*.f)
MYTMP= $(patsubst %.f,%.o, $(MYSRC))
MYOBJ= $(patsubst $(SRCDIR)/$(SUB)%,$(OBJDIR)%, $(MYTMP))
all: $(MYOBJ)
 
 
$(MYOBJ): $(OBJDIR)/%.o: $(SRCDIR)/$(SUB)/%.f
    $(FC) -c $(FFLAGS)  $< -o $@
 
clean:
    rm -f $(MYOBJ)
 
submit: